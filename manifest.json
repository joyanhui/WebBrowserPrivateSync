{
  "manifest_version": 3,
  "name": "WebBrowserPrivateSync",
  "version": "1.0",
  "description": "使用WebDAV同步浏览器书签",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "js/background.js"
  }
} 